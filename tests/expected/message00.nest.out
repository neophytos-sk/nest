        </decl>
        <code>
            rename struct.proxy struct._proxy

            nsp_alias {struct.proxy} {lambda} {target name} {
                with_fwd proxy nest [list with_proxy [gen_eval_name ${name}] ${target}] ${name} {
                    struct.proxy.target $target
                    struct.proxy.name $name
                }
            }
        </code>
